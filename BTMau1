#include <stdio.h>

// Hàm kiểm tra xem một số có phải là bội của 7 hay không
int laBoiCua7(int n) {
    return (n % 7 == 0);
}

// Hàm xuất các số nguyên có 2 chữ số và là bội của 7
void xuatSoBoiCua7() {
    printf("Cac so nguyen co 2 chu so va la boi cua 7 la:\n");
    for (int i = 10; i <= 99; i++) {
        if (laBoiCua7(i)) {
            printf("%d ", i);
        }
    }
    printf("\n");
}

// Hàm main
int main() {
    xuatSoBoiCua7(); // Gọi hàm để xuất các số
    return 0;
}
